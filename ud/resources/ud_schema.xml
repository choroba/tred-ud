  <type name="ud.misc.type">
    <list ordered="1"><cdata format="any"/></list>
  </type>

  <type name="ud.deps.type">
    <list ordered="1">
      <container>
        <attribute name="func"><cdata format="any"/></attribute>
        <cdata format="number"/>
      </container>
    </list>
  </type>

</pml_schema>
